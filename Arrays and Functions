const concept = ['arrays', 'can', 'be', 'mutated'];

function changeArr(arr){
  arr[3] = 'MUTATED';
}

changeArr(concept);


console.log(concept);

const concept = ['arrays', 'can', 'be', 'mutated'];

function changeArr(arr){
  arr[3] = 'MUTATED';
}

changeArr(concept);


console.log(concept);

function removeElement(newArr){
  return newArr.pop();
}

removeElement(concept);
console.log(concept);
