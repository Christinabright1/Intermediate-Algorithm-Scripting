//link list is a collection of node
//Node store a piece of data we can name it 
//store reference to the next node -> next
//create new Node 
class Node {
  constructor(val) {
    this.val = val;
    this.next = null;
  }
}
class SinglyList {
  constructor() {
    this.head = head;
    this.tail = tail;
    this.length = length;
  }
  push(val) {
    var newNode = new Node(val);
    if (!this.head) {
      this.head = newNode;
      this.tail = this.head;
    } else {
      this.tail.next = newNode;
      this.tail = newNode;
    
    }
      this.length++;
    return this;
  }
}


